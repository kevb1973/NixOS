// Binds
binds {
    // Launchers,
    Mod+Return            { spawn "kitty"; }
    Mod+Shift+Return      { spawn "nautilus"; }
    Mod+Z                 { spawn-sh "/home/kev/Code/dms-bin/dms ipc spotlight toggle"; }
    Mod+Tab               { toggle-overview; }
    Mod+KP_Multiply       { spawn "qalculate-gtk"; }
    Mod+N                 { spawn "~/bin/remind_gui"; }
    Mod+Shift+N           { spawn-sh "/home/kev/Code/dms-bin/dms ipc notepad toggle"; }
    Mod3+A                { spawn "bash" "-c" "pkill wlr-which-key || wlr-which-key"; }
    Mod3+E                { spawn "emacsclient" "-c"; }
    Mod3+Tab              { spawn "niri-window-switcher"; }

    // Utilities
    F10                   { spawn "/home/kev/NixOS/home/dots/bin/dotool-move-mouse.sh"; }
    Mod+c                 { spawn "copyq" "show"; }
    Mod+a                 { spawn-sh "/home/kev/Code/dms-bin/dms ipc notifications toggle"; }
    Mod+b                 { spawn-sh "/home/kev/Code/dms-bin/dms ipc bar toggle"; }
    Mod+e                 { spawn "/home/kev/NixOS/home/dots/bin/emoji-picker"; }
    Mod+M                 { spawn "~/bin/play_yt.sh"; }

    // Media

    // PC Volume
    XF86AudioRaiseVolume  { spawn-sh "/home/kev/Code/dms-bin/dms ipc audio increment 2"; }
    XF86AudioLowerVolume  { spawn-sh "/home/kev/Code/dms-bin/dms ipc audio decrement 2"; }
    XF86AudioMute         { spawn-sh "/home/kev/Code/dms-bin/dms ipc audio mute"; }

    // LGTV
    Mod+Delete            { spawn "/home/kev/NixOS/home/dots/bin/lgtv" "system" "turnOff";}
    Mod+Shift+Delete      { spawn "/home/kev/NixOS/home/dots/bin/lgtv" "wakeonlan"; }
    Mod+Prior             { spawn "/home/kev/NixOS/home/dots/bin/lgtv" "audio" "setMute" "true"; }
    Mod+Shift+Prior       { spawn "/home/kev/NixOS/home/dots/bin/lgtv" "audio" "setMute" "false"; }
    Mod+Next              { spawn "/home/kev/NixOS/home/dots/bin/lgtv" "audio" "volumeDown"; }
    Mod+Shift+Next        { spawn "/home/kev/NixOS/home/dots/bin/lgtv" "audio" "volumeUp"; }
    Mod+t                 { spawn "/home/kev/NixOS/home/dots/bin/lgtvgui"; }

    // MPRIS
    Mod+KP_Home           { spawn "playerctl" "play-pause"; }
    Mod+KP_Up             { spawn "playerctl" "previous"; }
    Mod+KP_Prior          { spawn "playerctl" "next"; }
    Mod+KP_Begin          { spawn "playerctld" "shift" "&&" "~/bin/currentPlayer"; }
    Mod+KP_Right          { spawn "playerctld" "unshift" "&&" "~/bin/currentPlayer"; }

    // Window Management
    Mod+Shift+1           { move-window-to-workspace 1; }
    Mod+Shift+2           { move-window-to-workspace 2; }
    Mod+Shift+3           { move-window-to-workspace 3; }
    Mod+Shift+4           { move-window-to-workspace 4; }
    Mod+Shift+5           { move-window-to-workspace 5; }
    Mod+Shift+6           { move-window-to-workspace 6; }
    Mod+Shift+7           { move-window-to-workspace 7; }
    Mod+Shift+8           { move-window-to-workspace 8; }
    Mod+Shift+9           { move-window-to-workspace 9; }
    Mod+Shift+Up          { move-window-up-or-to-workspace-up; }
    Mod+Shift+Down        { move-window-down-or-to-workspace-down; }
    Mod+F                 { fullscreen-window; }
    Mod+Space             { switch-focus-between-floating-and-tiling; }
    Mod+Shift+Space       { toggle-window-floating; }
    Mod+Shift+X           { close-window; }

    // Columns,
    Mod+Shift+Left        { move-column-left; }
    Mod+Shift+Right       { move-column-right; }
    // Mod+Shift+Down        { move-window-down; }
    // Mod+Shift+Up          { move-window-up; }
    Mod+Ctrl+1            { move-column-to-workspace 1; }
    Mod+Ctrl+2            { move-column-to-workspace 2; }
    Mod+Ctrl+3            { move-column-to-workspace 3; }
    Mod+Ctrl+4            { move-column-to-workspace 4; }
    Mod+Ctrl+5            { move-column-to-workspace 5; }
    Mod+Ctrl+6            { move-column-to-workspace 6; }
    Mod+Ctrl+7            { move-column-to-workspace 7; }
    Mod+Ctrl+8            { move-column-to-workspace 8; }
    Mod+Ctrl+9            { move-column-to-workspace 9; }
    Mod+Ctrl+Left         { consume-or-expel-window-left; }
    Mod+Ctrl+Right        { consume-or-expel-window-right; }

    // Floats
    // Not working yet..
    // Mod3+right            {move-floating-window -x 10; }
    // Mod3+Left             {move-floating-window -x -10; }
    // Mod3+up               {move-floating-window -y 10; }
    // Mod3+down             {move-floating-window -y -10; }

    // Focus
    Mod+Left              { focus-column-left-or-last; }
    Mod+Right             { focus-column-right-or-first; }
    // Mod+Up                { focus-window-up; }
    // Mod+Down              { focus-window-down; }
    Mod+Down              { focus-window-or-workspace-down; }
    Mod+Up                { focus-window-or-workspace-up; }
    Mod+1                 { focus-workspace 1; }
    Mod+2                 { focus-workspace 2; }
    Mod+3                 { focus-workspace 3; }
    Mod+4                 { focus-workspace 4; }
    Mod+5                 { focus-workspace 5; }
    Mod+6                 { focus-workspace 6; }
    Mod+7                 { focus-workspace 7; }
    Mod+8                 { focus-workspace 8; }
    Mod+9                 { focus-workspace 9; }
    Mod+slash             { center-column; }

    // Resize
    Mod+R                 { switch-preset-column-width; }
    Mod+W                 { toggle-column-tabbed-display; }
    Mod+Backspace         { maximize-column; }
    Mod+Shift+Backspace   { center-visible-columns; }
    Mod+Backslash         { toggle-windowed-fullscreen; }
    Mod+Shift+Backslash   { maximize-window-to-edges; }
    Mod+Minus             { set-column-width "-5%"; }
    Mod+Equal             { set-column-width "+5%"; }
    Mod+Shift+Minus       { set-window-height "-5%"; }
    Mod+Shift+Equal       { set-window-height "+5%"; }

    // Miscellaneous Binds
    Mod+s                 { screenshot; }
    // Mod+Shift+Slash       { show-hotkey-overlay; }
    Mod+q                 { spawn-sh "/home/kev/Code/dms-bin/dms ipc powermenu toggle"; }
    Mod+KP_Divide         { power-off-monitors; }

    // Mouse Binds
    Mod+WheelScrollUp     { focus-workspace-up; }
    Mod+WheelScrollDown   { focus-workspace-down; }

}
